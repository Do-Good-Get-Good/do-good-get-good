{
  "main": "index.js",
  "scripts": {
    "android": "npx react-native run-android --variant=devDebug --appIdSuffix=dev",
    "ios": "npx react-native run-ios --scheme 'srcDev' --simulator='iPhone 14 Pro Max'",
    "start": "react-native start",
    "test": "jest"
  },
  "transform": {
    "\\.js$": "src/node_modules/babel-jest"
  },
  "dependencies": {
    "@babel/preset-flow": "^7.14.5",
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-community/netinfo": "^8.2.0",
    "@react-native-firebase/app": "^14.8.0",
    "@react-native-firebase/auth": "^14.8.0",
    "@react-native-firebase/crashlytics": "^14.8.0",
    "@react-native-firebase/firestore": "^14.8.0",
    "@react-native-firebase/functions": "^14.7.0",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.1",
    "date-fns": "^2.24.0",
    "expo": "~42.0.1",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.68.1",
    "react-native-calendars": "^1.1282.0",
    "react-native-config": "^1.4.5",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-unimodules": "~0.14.5",
    "react-native-vector-icons": "^8.1.0",
    "react-test-renderer": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react-native": "^7.2.0",
    "jest": "^26.6.3"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./reports/coverage",
    "collectCoverageFrom": [
      "./screens/**",
      "./components/**",
      "index.js",
      "App.js",
      "./context/**"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 1
      }
    }
  },
  "private": true
}
